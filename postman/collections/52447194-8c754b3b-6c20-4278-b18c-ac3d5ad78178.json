		{
			"name": "Write Operations",
			"item": [
				{
					"name": "Create new post",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 201\", function () {",
									"    pm.response.to.have.status(201);",
									"});",
									"",
									"pm.test(\"Response has id property\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.have.property('id');",
									"});"
								],
								"type": "text/javascript"
							}
						}
					],
					"id": "52447194-d889f3b0-dfee-4c70-9072-9f704b77bda7",
					"protocolProfileBehavior": {
						"disableBodyPruning": true
					},
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"title\": \"My New Post\",\n  \"body\": \"This is the content of my new post. POST requests are used to create new resources.\",\n  \"userId\": 1\n}"
						},
						"url": {
							"raw": "https://jsonplaceholder.typicode.com/posts",
							"protocol": "https",
							"host": [
								"jsonplaceholder",
								"typicode",
								"com"
							],
							"path": [
								"posts"
							]
						},
						"description": "Creates a new post by sending JSON data in the request body. POST requests are used to create new resources on the server. Notice the Content-Type header and the JSON body with title, body, and userId fields."
					},
					"response": []
				},
